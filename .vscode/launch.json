    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "env": {
                "DATE_YEAR": "2061",
                "DATE_MONTH": "1",
                "DATE_DAY": "1",
                "SLACK_WEBHOOK_URL": "SOME_WEBHOOK_URL"
            }
        }
    ]
}
